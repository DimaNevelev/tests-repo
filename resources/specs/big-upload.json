      "recursive": "true"
    },
    {
      "pattern": "resources/a/*",
      "target": "CliTestsSpec/flat_nonrecursive/",
      "flat": "true",
      "recursive": "false"
    },
    {
      "pattern": "resources/a/*",
      "target": "CliTestsSpec/nonflat_recursive/",
      "flat": "false",
      "recursive": "true"
    },
    {
      "pattern": "resources/a/*",
      "target": "CliTestsSpec/nonflat_nonrecursive/",
      "flat": "false",
      "recursive": "false"
    },
    {
      "pattern": "resources/a/*",
      "target": "CliTestsSpec/props/",
      "props": "p1=v1;p2=v2"
    },
    {
      "pattern": "resources/a/(.*)",
      "target": "CliTestsSpec/regexp/",
